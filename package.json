{
  "name": "rts",
  "version": "3.0.0",
  "description": "race tracking system",
  "main": "src/electron-start.js",
  "scripts": {
    "ub": "node scripts/updatebin.js",
    "test": "jest",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "electron": "electron .",
    "server": "tsc -p tsconfig.server.json",
    "server-watch": "tsc -p tsconfig.server.json -w",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dev": "nf start -p 3000"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@blueprintjs/core": "3.18.1",
    "@blueprintjs/icons": "^3.10.0",
    "@blueprintjs/select": "^3.10.0",
    "antd": "3.23.2",
    "chart.xkcd-react": "0.0.6",
    "electron": "2.0.7",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "react-scripts": "3.1.1",
    "redux": "4.0.4",
    "sqlite3": "4.0.9",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/jest": "24.0.18",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "7.1.2",
    "@types/sqlite3": "3.1.5",
    "electron-builder": "21.1.5",
    "foreman": "3.0.1",
    "node-sass": "4.12.0",
    "ts-jest": "24.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/*.{ts,tsx}",
      "!src/definitions/**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
